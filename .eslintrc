{
    "root": true,
    "extends": "eslint:recommended",
    "globals": {
      "wp": true
    },
    "env": {
      "amd": true,
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "parser": "babel-eslint"
  }